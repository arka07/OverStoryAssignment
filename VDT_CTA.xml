<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Testing Suite">
  <listeners>
		<listener class-name="com.ericsson.edos.dopuatautomation.util.ListenersTest" />
		<listener class-name="com.ericsson.edos.dopuatautomation.util.ExtentReportNG" />
	</listeners>

	<test name=" LandingPageTest">
		<classes>
			<class name="com.ericsson.edos.dopuatautomation.testcases.LandingpageTest" />
		</classes>
	</test>
	
	<test name="LoginPageTest">
		<classes>
			<class name="com.ericsson.edos.dopuatautomation.testcases.LoginPageTest" />
		</classes>
	</test>
	
	<test name="ApplicationPageTest">
	<classes>
			<class name="com.ericsson.edos.dopuatautomation.testcases.applicationTest" >
			 <methods>
              <include name="VerifyCTALink" />
             </methods>
            </class>	 
	   
		</classes>
	</test>
	
		<test name="CallTerminationAnalysisTest">
		<classes>
			<class name="com.ericsson.edos.dopuatautomation.testcases.CallTerminationAnalysisTest">
              <methods>
              <include name="VerifyCellFilterData" />
              <include name="VerifyCellFilterSearch" />
              <include name="VerifyCTACallsDataGridTable"/>
              <include name="VerifyNonTableComponents"/>
              <include name="VerifyOpenCallDetails"/>
              <include name="ValidateCallEvents"/>
              <include name="VerifyCallsPlottedonMap"/>
              <include name="VerifyCallsDetailsfrommap"/>
             </methods>
             </class>
		</classes>
	</test>
	
	

</suite> 
